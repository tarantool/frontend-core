    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "build_module": "node ./scripts/build_module.js",
    "test": "node ./scripts/test.js test --env=jsdom",
    "test_once": "node ./scripts/test.js test --env=jsdom --once --silent",
    "eslint": "./node_modules/.bin/eslint src",
    "lint": "npm run eslint",
    "fix": "./node_modules/.bin/eslint --fix src",
    "flow": "./node_modules/.bin/flow",
    "prepublishOnly": "npm ci && npm run build_module",
    "cy:run": "cypress run"
  },
  "devDependencies": {
    "@tarantool.io/lua-bundler-webpack-plugin": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "1.5.17",
    "axios": "0.21.1",
    "copy-webpack-plugin": "5.1.2",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-emotion": "^9.2.6",
    "eslint-plugin-prettier": "3.1.4",
    "graphql-tag": "^2.10.1",
    "mock-local-storage": "^1.1.11",
    "nock": "^11.7.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "mock-local-storage"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
